{
  "$schema": "../node_modules/@tauri-apps/cli/schemas/desktop-capabilities.json",
  "identifier": "default",
  "description": "Default capabilities for Liike (fs + shell for render pipeline).",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "fs:write-files"
    },
    {
      "identifier": "fs:read-files"
    },
    {
      "identifier": "fs:read-dirs"
    },
    {
      "identifier": "fs:allow-mkdir",
      "allow": [
        {
          "path": "$TEMP/**"
        },
        {
          "path": "$DOWNLOAD/**"
        },
        {
          "path": "$HOME/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-remove",
      "allow": [
        {
          "path": "$TEMP/**"
        },
        {
          "path": "$DOWNLOAD/**"
        },
        {
          "path": "$HOME/**"
        }
      ]
    },
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "$TEMP/**"
        },
        {
          "path": "$DOWNLOAD/**"
        },
        {
          "path": "$DESKTOP/**"
        },
        {
          "path": "$HOME/**"
        }
      ]
    },
    {
      "identifier": "shell:allow-spawn"
    },
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "unzip",
          "cmd": "/usr/bin/unzip",
          "args": true
        },
        {
          "name": "open",
          "cmd": "/usr/bin/open",
          "args": true
        },
        {
          "name": "explorer",
          "cmd": "explorer",
          "args": true
        },
        {
          "name": "xdg-open",
          "cmd": "xdg-open",
          "args": true
        }
      ]
    },
    {
      "identifier": "shell:allow-stdin-write"
    },
    {
      "identifier": "shell:allow-open",
      "allow": [
        {
          "path": "$DOWNLOAD/**"
        },
        {
          "path": "$HOME/**"
        }
      ]
    },
    "dialog:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://tavggalbjqwdfobiaoec.supabase.co/*"
        },
        {
          "url": "https://fonts.googleapis.com/*"
        },
        {
          "url": "https://fonts.gstatic.com/*"
        }
      ]
    },
    "os:default"
  ]
}